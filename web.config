<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <clear />
                <add value="index.html" />
            </files>
        </defaultDocument>
        <rewrite>
          <rules>
            <rule name="Rewrite squarespace feed to jekyll feed">
              <match url="^powershell\?format=RSS" />
              <action type="Rewrite" url="feed.xml" />
            </rule>
          </rules>
        </rewrite>
    </system.webServer>
</configuration>
